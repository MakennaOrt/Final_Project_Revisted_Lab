import sys
from PyQt5.QtWidgets import QApplication
from vote_menu import VoteMenu
def main():
   app = QApplication(sys.argv)
   total_votes = {'Diego': 0, 'Allison': 0, 'Cameron': 0}
   vote_menu = VoteMenu(total_votes)


   vote_menu.show()
   sys.exit(app.exec_())


if __name__ == '__main__':
   main()
